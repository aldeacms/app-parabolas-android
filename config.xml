<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "org.siervofiel.parabolas.test"
		versionCode="10"
        version   = "1.0.0">

		<name>Parabolas de Jesus</name>

		<description>
			Parabolas de Jesus para Android
		</description>

		<author href="https://www.siervofiel.org" email="info@siervofiel.org">
			Ministerio Siervo Fiel
		</author>
		
		<plugin name="InAppBrowser" value="CDVInAppBrowser" />

		<gap:platform name="android" />

		<preference name="android-minSdkVersion" value="10" />
		<preference name="permissions" value="none"/>		
		<preference name="phonegap-version" value="3.0.0" />
		<preference name="orientation" value="portrait" />
		<preference name="webviewbounce" value="false" />
		<preference name="stay-in-webview" value="false" />

		<icon src="img/icon-72.png" />
		<icon src="img/icon-36.png" gap:platform="android" gap:density="ldpi" />
		<icon src="img/icon-48.png" gap:platform="android" gap:density="mdpi" />
		<icon src="img/icon-72.png" gap:platform="android" gap:density="hdpi" />
		<icon src="img/icon-96.png" gap:platform="android" gap:density="xhdpi" />
		<icon src="img/icon-144.png" gap:platform="android" gap:density="xxhdpi" />

		<gap:splash src="splash.png" />
		

	</widget>
